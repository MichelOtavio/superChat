                        baseLiveAgentContentURL: 'https://c.la4-c1-ia5.salesforceliveagent.com/content',
                        deploymentId: '5725e0000011L6r',
                        buttonId: '5735e0000011LPx',
                        baseLiveAgentURL: 'https://d.la4-c1-ia5.salesforceliveagent.com/chat',
                        eswLiveAgentDevName: 'chaters',
                        isOfflineSupportEnabled: false
                    }
                );
            };
        
            if (!window.embedded_svc) {
                var s = document.createElement('script');
                s.setAttribute('src', 'https://elevendevelopersystem-dev-ed.my.salesforce.com/embeddedservice/5.0/esw.min.js');
                s.onload = function() {
                    initESW(null);
                };
                document.body.appendChild(s);
            } else {
                initESW('https://service.force.com');
            }
        
        </script>



